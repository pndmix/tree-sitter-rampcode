{
  "name": "tree-sitter-rampcode",
  "version": "0.2.0",
  "description": "Rampcode grammar for tree-sitter",
  "keywords": [
    "tree-sitter",
    "parser",
    "rampcode"
  ],
  "main": "index.js",
  "repository": "https://github.com/pndmix/tree-sitter-rampcode",
  "author": "Reo Matsumoto",
  "license": "MIT",
  "dependencies": {
    "nan": "^2.14.0",
    "prebuild-install": "^5.3.0"
  },
  "devDependencies": {
    "prebuild": "^9.0.0",
    "tree-sitter-cli": "^0.15.2"
  },
  "scripts": {
    "install": "prebuild-install || node-gyp rebuild",
    "prebuild": "prebuild -r electron -t 1.6.0 -t 1.7.0 -t 1.8.0 -t 2.0.0 -t 3.0.0 -t 4.0.0 --strip --verbose",
    "prebuild:upload": "prebuild --upload-all",
    "build": "tree-sitter generate && node-gyp build",
    "test": "tree-sitter test"
  }
}
